extends layout

block content
  .w3-card-4
    h1 Welcome, #{user.displayName}!
    img(src=user.picture)
    if user.subjects.psychology === true
      p You got premium!
    else
      p you're a loser
    - var values = [user.subjects];
    ul
      each val in values
        li= val
      else
        li You haven't registered for any subjects